{
    "name": "touhidurabir/command-validator",
    "description": "A simple laravel package to validate console commands arguments and options.",
    "type": "library",
    "keywords": [
        "touhidurabir",
        "laravel",
        "laravel-console",
        "command-validator",
        "console",
        "commands",
        "validator",
        "validation"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Touhidurabir\\CommandValidator\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Touhidurabir\\CommandValidator\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Touhidur Rahman",
            "email": "abircse06@gmail.com"
        }
    ],
    "require": {
        "illuminate/contracts": "^8.61",
        "illuminate/support": "^8.61",
        "symfony/console": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.21"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Touhidurabir\\CommandValidator\\CommandValidatorServiceProvider"
            ]
        }
    }
}
